{
  "name": "claude-code-ultimate-web-app",
  "version": "1.0.0",
  "description": "Modern web application template with Claude Code Ultimate integration",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "claude:setup": "claude doctor && node scripts/setup-claude.js",
    "claude:agents": "sc spawn code-reviewer && sc spawn test-generator"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "axios": "^1.6.0",
    "zustand": "^4.4.6",
    "react-query": "^3.39.3",
    "react-hook-form": "^7.47.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "jest": "^29.7.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.4"
  },
  "claude": {
    "agents": ["code-reviewer", "test-generator", "doc-generator"],
    "mcpServers": ["context7", "sequential", "playwright", "magic"],
    "templates": ["component", "page", "api", "test"],
    "hooks": {
      "pre-commit": ["lint", "test"],
      "pre-push": ["build"]
    }
  }
}
